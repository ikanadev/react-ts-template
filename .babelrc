{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-typescript",
    "@babel/preset-react"
  ],
  "plugins": [
    // plugin-proposal-decorators is only needed if you're using experimental decorators in TypeScript
    [
      "@babel/plugin-proposal-decorators",
      {
        "decoratorsBeforeExport": true
      }
    ],
    "react-hot-loader/babel"
  ]
}